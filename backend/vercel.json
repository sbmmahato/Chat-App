{
    "version": 2,
    "builds": [
    {
    "src": "./index.js",
    "use": "@vercel/node"
    }
    ],
    "routes": [
    {
    "src": "/",
    "dest": "/",
    "methods":["GET","POST","PUT","PATCH","DELETE","OPTIONS"]
    },
    {
        "src": "/ui/:username",
        "dest": "/"
        },
        {
            "src": "/registration",
            "dest": "/"
            },
            {
                "src": "/login",
                "dest": "/"
                },
                {
                    "src": "/friendReq/:username",
                    "dest": "/"
                    }
    ]
    }